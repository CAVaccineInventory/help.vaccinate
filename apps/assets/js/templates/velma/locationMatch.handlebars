<ul>
  {{#each candidates }}
  <li>
    <div class="container">
      <div class="row">
        <div class="col-6">
          {{ this.name }} {{ this.full_address }}<br />
          {{ this.latitude }} , {{ this.longitude}} <br />
          <a href="{{ this.website }}" target="_blank">{{ this.website }}</a>
          <input type="button" class="btn btn-primary" value="Matches this one!" />
        </div>
        <div class="col-6">
          <div class="map" style="height: 200px" id="map-{{this.id}}">Map goes here</div>
        </div>
      </div>
    </div>
  </li>
  {{/each}}
</ul>
