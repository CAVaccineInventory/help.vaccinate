<div class="location">
    <div class="h6" id="location-name">{{locationName}}</div>
    <address class="small" id="location-address">{{locationAddress}}</address>

    <div class="small" id="location-hours">{{locationHours}}</div>
<div class="d-grid gap-2 d-md-flex justify-content-md-end">
    <a target="_blank" class="text-muted small" href="https://airtable.com/shrY44NvEjHBscrOH?prefill_Location={{locationId}}&prefill_Type=Incorrect+contact+information">Report an error</a>
</div>
<div class="countyInfo">
<span id="locationCountyName">{{countyName}}</span>
    <div class="small">{{countyInfo}}</div>
</div>

</div>
