{
  "name": "apps",
  "version": "0.0.1",
  "description": "The apps for help.vaccinate.ca",
  "main": "index.html",
  "devDependencies": {
    "@auth0/auth0-spa-js": "^1.13.6",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@webpack-cli/init": "^1.1.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "eslint": "^7.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-es": "github:mysticatea/eslint-plugin-es",
    "foreman": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "webpack": "^5.21.0",
    "webpack-cli": "^4.5.0"
  },
  "scripts": {
    "clean": "rimraf _site",
    "serve:webpack": "webpack --mode development --watch",
    "serve:eleventy": "ELEVENTY_ENV=development eleventy --serve",
    "serve": "npm-run-all clean --parallel serve:*",
    "build:webpack": "webpack --mode production",
    "build:eleventy": "ELEVENTY_ENV=production eleventy",
    "build": "run-s clean build:*",
    "lint": "prettier --check assets/**/*.js '**/*.html' && eslint assets/js",
    "lint:fix": "prettier --write assets/**/*.js **/*.html && eslint --fix assets/js"
  },
  "author": "vaccinateca.com",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "core-js": "^3.8.3",
    "custom-event-polyfill": "^1.0.7",
    "handlebars-loader": "^1.7.1",
    "luxon": "^1.25.0",
    "regenerator-runtime": "^0.13.7",
    "whatwg-fetch": "^3.5.0"
  }
}
